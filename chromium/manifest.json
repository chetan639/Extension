{
    "manifest_version": 3,
    "name": "LogInfo from stackoverflow",
    "version": "1.0",
    "action": {
        "default_icon": "makar_sankranti.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "background":{
        "service_worker": "background.js"
    },
    "permissions": ["tabs"]
}