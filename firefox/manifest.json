{
    "manifest_version": 2,
    "name": "LogInfo from stackoverflow",
    "version": "1.0",
    "browser_action": {
        "default_icon": "makar_sankranti.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "background":{
        "scripts":["background.js"]
    },
    "permissions": ["tabs"]
}